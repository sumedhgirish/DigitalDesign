    .port_info 0 /OUTPUT 1 "o";
    .port_info 1 /INPUT 1 "x";
v0x555905604a20_0 .net "o", 0 0, L_0x555905604ff0;  1 drivers
o0x7f4f0679f168 .functor BUFZ 1, C4<z>; HiZ drive
v0x555905604ac0_0 .net "x", 0 0, o0x7f4f0679f168;  0 drivers
L_0x555905604ff0 .reduce/nor o0x7f4f0679f168;
S_0x5559055f33e0 .scope module, "or2" "or2" 2 16;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "o";
    .port_info 1 /INPUT 1 "x";
    .port_info 2 /INPUT 1 "y";
o0x7f4f0679f228 .functor BUFZ 1, C4<z>; HiZ drive
o0x7f4f0679f258 .functor BUFZ 1, C4<z>; HiZ drive
L_0x555905605090 .functor OR 1, o0x7f4f0679f228, o0x7f4f0679f258, C4<0>, C4<0>;
v0x555905604ba0_0 .net "o", 0 0, L_0x555905605090;  1 drivers
v0x555905604c60_0 .net "x", 0 0, o0x7f4f0679f228;  0 drivers
v0x555905604d20_0 .net "y", 0 0, o0x7f4f0679f258;  0 drivers
    .scope S_0x5559055f0c10;
T_0 ;
    %vpi_call 2 3 "$display", "Hello, World!" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5559055f0da0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5559056047b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555905604850_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5559056047b0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x555905604850_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5559055f0da0;
T_2 ;
    %delay 2, 0;
    %vpi_call 3 19 "$display", "The output of and2 %b %b: %b", v0x5559056047b0_0, v0x555905604850_0, v0x555905604920_0 {0 0 0};
