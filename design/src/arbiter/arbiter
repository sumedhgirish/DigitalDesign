v0x564b93063170_0 .net "out1", 0 0, v0x564b93062a50_0;  1 drivers
v0x564b93063260_0 .var "reset", 0 0;
S_0x564b9304d910 .scope module, "U0" "arbiter" 2 26, 3 2 0, S_0x564b9304d780;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "req_0";
    .port_info 3 /INPUT 1 "req_1";
    .port_info 4 /OUTPUT 1 "grt_0";
    .port_info 5 /OUTPUT 1 "grt_1";
v0x564b9303bc30_0 .net "clk", 0 0, v0x564b93062e40_0;  1 drivers
v0x564b93062990_0 .var "grt_0", 0 0;
v0x564b93062a50_0 .var "grt_1", 0 0;
