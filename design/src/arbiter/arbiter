v0x564b93062bb0_0 .net "req_1", 0 0, v0x564b93062fa0_0;  1 drivers
v0x564b93062cc0_0 .net "reset", 0 0, v0x564b93063260_0;  1 drivers
E_0x564b9304ad90 .event posedge, v0x564b93062cc0_0, v0x564b9303bc30_0;
    .scope S_0x564b9304d910;
T_0 ;
    %wait E_0x564b9304ad90;
    %load/vec4 v0x564b93062cc0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x564b93062990_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x564b93062a50_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x564b93062af0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x564b93062990_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x564b93062a50_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x564b93062bb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x564b93062990_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x564b93062a50_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x564b93062990_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x564b93062a50_0, 0;
T_0.5 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x564b9304d780;
T_1 ;
    %vpi_call 2 6 "$monitor", "req0=%b,req1=%b,gnt0=%b,gnt1=%b", v0x564b93062f00_0, v0x564b93062fa0_0, v0x564b930630a0_0, v0x564b93063170_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564b93062e40_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564b93063260_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564b93062f00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564b93062fa0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564b93063260_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564b93063260_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564b93062f00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564b93062f00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564b93062fa0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564b93062fa0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 3, 0, 2;
    %split/vec4 1;
    %store/vec4 v0x564b93062fa0_0, 0, 1;
    %store/vec4 v0x564b93062f00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 2;
    %split/vec4 1;
    %store/vec4 v0x564b93062fa0_0, 0, 1;
    %store/vec4 v0x564b93062f00_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x564b9304d780;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x564b93062e40_0;
    %nor/r;
    %store/vec4 v0x564b93062e40_0, 0, 1;
